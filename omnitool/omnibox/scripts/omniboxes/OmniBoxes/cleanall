from manageinstance import delete_instance
import os
import shutil

# for every folder of the form omnibox-<number> in this dir delete the docker container (leave folder)
for folder in os.listdir("."):
    if folder.startswith("omnibox-"):
        delete_instance(int(folder.split("-")[1]))
        # uncomment below to also delete the folder
        shutil.rmtree(folder)
